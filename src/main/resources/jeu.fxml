<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onKeyPressed="#handleKeyPressed"
	onKeyReleased="#handleKeyReleased" fx:id="scene" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="540.0" prefWidth="960.0"
	style="-fx-background-color: black;"
	xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.intech.bindingofisaac.controller.Salle1Controller">
	<children>
		<ImageView fitHeight="540.0" fitWidth="960.0">
			<image>
				<Image url="@img.jpg" />
			</image>
		</ImageView>
		<ImageView fx:id="imageIsaac" fitHeight="64.0"
			fitWidth="58.0" pickOnBounds="true" preserveRatio="true" />
		
		<Button mnemonicParsing="false"
			style="-fx-background-color: transparent;" />
			
			<fx:include fx:id="ennemies" source="enemies.fxml"/>
	</children>

</AnchorPane>
